@using Jarboo.Admin.Web.Models.Documentation
@model IEnumerable<Jarboo.Admin.DAL.Entities.Documentation>

@{
    ViewBag.Title = "Documentations";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@(new BreadCrumbsBuilder()
    .Add("Documentations")
    .Done())

<p>
    <a class="btn btn-success" href="@Url.Action(MVC.Documentations.Create())">
        <span class="glyphicon glyphicon-plus" aria-hidden="true"></span> Add documentation
    </a>
</p>

@Html.Action(MVC.Documentations.ActionNames.List, MVC.Documentations.Name, new { showProject = true})